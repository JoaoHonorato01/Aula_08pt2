package Estação;
import java.util.Scanner;
public class EstaçõesDoAno {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		System.out.print("digite um numero de 1 a 12");
		int Estacoes = scanner.nextInt();
		
		switch(Estacoes) {
		case 1,2,3:
			System.out.print("VERAO");
		break;
		case 4,5,6:
			System.out.print("OUTONO");
		break;
		case 7,8,9:
			System.out.print("INVERNO");
		break;
		case 10,11,12:
			System.out.print("PRIMAVERA");
		break;
		
		} scanner.close();
	}

}
